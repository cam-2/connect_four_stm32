#ifndef INC_GAMEDATA_H_
#define INC_GAMEDATA_H_

struct buttonBounds_t {

	uint16_t xLower;
	uint16_t xUpper;
	uint16_t yLower;
	uint16_t yUpper;
};

struct coin_t {

	uint16_t xPos;
	uint16_t yPos;
	uint16_t radius;
	uint16_t color;
};

#endif /* INC_GAMEDATA_H_ */
