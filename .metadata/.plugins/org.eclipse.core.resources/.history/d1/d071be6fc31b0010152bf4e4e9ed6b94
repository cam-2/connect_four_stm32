#ifndef INC_GAMEDATA_H_
#define INC_GAMEDATA_H_

struct buttonBounds_t {
	uint16_t xLower;
	uint16_t xUpper;
	uint16_t yLower;
	uint16_t yUpper;
};

struct coin_t {
	uint8_t xLower;
	uint8_t xUpper;
	uint8_t yLower;
	uint8_t yUpper;
};

#endif /* INC_GAMEDATA_H_ */
